CREATE TABLE public.sto_company
(
  company_id bigint NOT NULL DEFAULT nextval('sto_company_company_id_seq'::regclass),
  name character varying(500),
  total_years_activity bigint,
  point bigint,
  domain character varying(255),
  listed_date date,
  symbol character(255),
  status character(255),
  CONSTRAINT sto_company_pkey PRIMARY KEY (company_id)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE public.sto_company
  OWNER TO postgres;


